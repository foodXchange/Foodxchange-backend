{
  "name": "foodxchange-backend",
  "version": "1.0.0",
  "description": "FoodXchange B2B marketplace backend",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "dev:fast": "tsx watch src/server-fast.ts",
    "build": "rimraf dist && tsc",
    "postbuild": "copyfiles -u 1 \"src/**/*.json\" \"src/**/*.html\" \"src/**/*.txt\" dist/ && copyfiles package.json web.config .env* dist/",
    "build:watch": "tsc --watch",
    "build:docker": "docker build -t foodxchange-backend .",
    "typecheck": "tsc --noEmit",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "test:unit": "jest --testPathPattern=unit --detectOpenHandles",
    "test:integration": "jest --testPathPattern=integration --detectOpenHandles --forceExit",
    "test:e2e": "jest --testPathPattern=e2e --detectOpenHandles --forceExit --runInBand",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist logs coverage",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f foodxchange-backend",
    "health": "curl -f http://localhost:5000/health || echo 'Health check failed'"
  },
  "keywords": [
    "foodxchange",
    "b2b",
    "marketplace",
    "food",
    "compliance"
  ],
  "author": "FoodXchange Team",
  "license": "ISC",
